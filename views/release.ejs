<div class="release release-<%=: release.id | normalize %> activated" data-releaseid="<%= release.id %>">
	<div class="albumart">
		<img src="" />
	</div>
	<div class="buttons">
		<button class="select" type="button">Select</button> 
		<button class="albumartok" type="button">Albumart OK</button>
	</div>
	<div class="info">
		<div class="meta">
			<span class="id"><%= release.id %></span>
			<span class="title"><%= release.title %></span>
			<span class="date"><%= release.date %></span>
			<span class="country"><%= release.country %></span>
			<span class="barcode"><%= release.barcode %></span>
			<span class="packaging"><%= release.packaging %></span>
		</div>
		<div class="labelinfos">
		<% for (var i = 0; i < release.labelInfo.length; i++) { %>
			<div class="labelinfo">
				<span class="catalognumber"><%= release.labelInfo[i].catalogNumber %></span>
				<% if (release.labelInfo[i].label) { %>
				<span class="label"><%= release.labelInfo[i].label.name %></label>
				<% } %>
			</div>
		<% } %>
		</div>
		<div class="mediums">
		<% for (var i = 0; i < release.mediums.length; i++) { %>
			<% var medium = release.mediums[i]; %>
			<div class="medium pos<%= medium.position %> <% for(var ii = 0; ii < medium.discs.length; ii++) { %> disc-<%=: medium.discs[ii].id | normalize %> <% } %>">
				<span class="discid">-----</span>
				<span class="position"><%= medium.position %></span>
				<span class="title"><%= medium.title %></span>
				<span class="format"><%= medium.format %></span>
				<span class="tracks"><%= medium.tracks.length %> tracks</span>

				<div class="tracks">
				<% for (var ii = 0; ii < medium.tracks.length; ii++) { %>
					<div class="track">
						<span class="position"><%= medium.tracks[ii].position %></span>
						<span class="step metadata">Metadata</span>
						<span class="step vorbiscomment">VorbisComment</span>
						<span class="step mp3">MP3</span>
						<span class="step ogg">OGG</span>
					</div>
				<% } %>
				</div>
			</div>
		<% } %>
		</div>
	</div>
</div>

<script type="text/javascript">
(function ($) {
})(jQuery);
</script>
